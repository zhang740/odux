{
  "name": "odux",
  "version": "0.1.4",
  "description": "observable redux",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf -- dist lib",
    "build:prod": "tsc && babel lib --out-dir lib",
    "build:dist": "webpack --progress",
    "build:test": "rimraf -- __tests__ && tsc -p tsconfig.test.json",
    "test": "tsc && npm run build:test && ava -v -s",
    "test:prod": "npm run build:prod && npm run build:test && ava -v -s",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "reinstall": "rimraf -- node_modules && npm install",
    "prepare": "tsc -v && npm run clean && npm run build:prod && npm run build:dist",
    "postinstall": "node scripts/published.js"
  },
  "ava": {
    "files": [
      "__tests__/*.js"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib"
  ],
  "keywords": [
    "react",
    "redux",
    "observable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/odux.git"
  },
  "author": "zhang740",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhang740/odux/issues"
  },
  "homepage": "https://github.com/zhang740/odux#readme",
  "dependencies": {
    "power-di": "^1.2.0",
    "redux": "^3.x"
  },
  "devDependencies": {
    "@types/react": "^16.0.21",
    "@types/react-test-renderer": "^16.0.0",
    "ava": "^0.23.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-cli": "^6.26.0",
    "ts-loader": "^3.1.1",
    "coveralls": "^3.0.0",
    "nyc": "^11.3.0",
    "react": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  }
}
