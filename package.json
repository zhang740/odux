{
  "name": "odux",
  "version": "0.0.7",
  "description": "observable redux",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf -- dist",
    "build:prod": "rimraf -- dist && tsc",
    "build:test": "rimraf -- __tests__ && tsc -p tsconfig.test.json",
    "test": "tsc && npm run build:test && ava -v -s",
    "test:prod": "npm run build:prod && npm run build:test && ava -v -s",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "reinstall": "rimraf -- node_modules && npm install",
    "prepublish": "npm run build:prod"
  },
  "ava": {
    "files": [
      "__tests__/*.js"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "simple.js",
    "simple.d.ts",
    "dist"
  ],
  "keywords": [
    "react",
    "redux",
    "observable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/odux.git"
  },
  "author": "zhang740",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhang740/odux/issues"
  },
  "homepage": "https://github.com/zhang740/odux#readme",
  "dependencies": {
    "power-di": "^1.0.8",
    "redux": "^3.7.1",
    "react-redux": "^5.0.5",
    "@types/react-redux": "^4.4.45"
  },
  "devDependencies": {
    "@types/react": "^15.0.34",
    "@types/react-test-renderer": "^15.5.1",
    "ava": "^0.20.0",
    "coveralls": "^2.13.1",
    "nyc": "^11.0.3",
    "react": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  }
}