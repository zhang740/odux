{
  "name": "odux",
  "version": "0.0.9",
  "description": "observable redux",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf -- dist lib",
    "build:prod": "tsc && babel lib --out-dir lib",
    "build:dist": "webpack --progress",
    "build:test": "rimraf -- __tests__ && tsc -p tsconfig.test.json",
    "test": "tsc && npm run build:test && ava -v -s",
    "test:prod": "npm run build:prod && npm run build:test && ava -v -s",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "reinstall": "rimraf -- node_modules && npm install",
    "prepublish": "npm run clean && npm run build:prod && npm run build:dist"
  },
  "ava": {
    "files": [
      "__tests__/*.js"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts",
    "simple.js",
    "simple.d.ts",
    "lib"
  ],
  "keywords": [
    "react",
    "redux",
    "observable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/odux.git"
  },
  "author": "zhang740",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhang740/odux/issues"
  },
  "homepage": "https://github.com/zhang740/odux#readme",
  "dependencies": {
    "power-di": "^1.0.8",
    "redux": "^3.7.1",
    "react-redux": "^5.0.5",
    "@types/react-redux": "^4.4.45"
  },
  "devDependencies": {
    "@types/react": "^15.6.0",
    "@types/react-test-renderer": "^15.5.1",
    "ava": "^0.21.0",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-cli": "^6.24.1",
    "ts-loader": "^2.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "nyc": "^11.1.0",
    "react": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1"
  }
}
